
currentslot = turtle.getSelectedSlot()

	if nextslot > 1 then
		previousslot = nextslot-1
	else
		previousslot = 16
	
	end
	
	continue = true
	
	while continue do
	
		if currentslot < 16 then
			nextslot=currentslot+1
		else
			nextslot=1
	end
	
		while turtle.getItemCount() == 0 do
			turtle.suckUp()
			turtle.select(nextslot)
		
			if turtle.getItemCount() > 0 then
				turtle.dropUp()
				sleep(600)
			else
				turtle.select(nextslot)
			end
		end
	end